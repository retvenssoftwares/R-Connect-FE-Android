<resources xmlns:tools="http://schemas.android.com/tools">
    <string name="app_name">ChannelManager</string>

    <dimen name="edittextwidth">400sp</dimen>
    <dimen name="EditTextDemo">275sp</dimen>
    <dimen name="HeadingSize">21sp</dimen>
    <dimen name="boldHeading">27sp</dimen>
    <dimen name="SubHeadingSize">23sp</dimen>
    <dimen name="subTitle">15sp</dimen>
    <dimen name="smallText">13sp</dimen>
    <dimen name="hintSize">10dp</dimen>
    <dimen name="bookingHeading">18sp</dimen>
    <dimen name="FullEditText">550sp</dimen>
    <dimen name="headingText">18sp</dimen>
    <dimen name="mediumHeadingText">16sp</dimen>
    <dimen name="subheadingText">14sp</dimen>
    <!-- TODO: Remove or change this placeholder text -->
    <string name="hello_blank_fragment">Hello blank fragment</string>


    <style name="NavigationViewStyle" parent="Widget.Design.NavigationView">
        <item name="android:textSize">20sp</item>
        <item name="fontFamily">@font/inter</item>
        <item name="android:textFontWeight" tools:targetApi="p">600</item>
        <!-- set the desired text size -->
    </style>

    <string name="navigation_drawer_close" translatable="false">Close Drawer</string>
    <string name="navigation_drawer_open" translatable="false">Open Drawer</string>


    <style name="RoundedCornerImage">
        <item name="cornerSize">50%</item>
    </style>

    <style name="MyDialogTheme" parent="Theme.AppCompat.Dialog">
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="PanoramaView">
        <item name="cornerSize">15%</item>
        <item name="android:background">@color/primarybackground</item>
    </style>

    <style name="FullImageDisplay">
        <item name="cornerSize">10%</item>
    </style>

    <declare-styleable name="PanoramaImageView">
        <attr name="piv_enablePanoramaMode" format="boolean" />
        <attr name="piv_invertScrollDirection" format="boolean" />
        <attr name="piv_show_scrollbar" format="boolean" />
    </declare-styleable>


</resources>
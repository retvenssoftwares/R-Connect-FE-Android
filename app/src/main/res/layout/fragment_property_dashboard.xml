<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".PropertyDashboard.PropertyDashboardFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/images_frame"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/png_hotel6"
            android:scaleType="fitXY"
            />


        <LinearLayout
            android:id="@+id/vertical_linear"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:orientation="vertical"
            android:gravity="center"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:rotation="90"
                android:layout_gravity="center"
                android:layout_marginStart="10dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="tanya"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="@dimen/mediumHeadingText"
                    android:textColor="@color/primarybackground"
                    />

                <View
                    android:layout_width="50dp"
                    android:layout_height="3dp"
                    android:background="#F44336"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:rotation="90"
                android:layout_gravity="center"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="helen"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="@dimen/mediumHeadingText"
                    android:textColor="@color/subtitle"
                    />

                <View
                    android:layout_width="50dp"
                    android:layout_height="3dp"
                    android:background="@drawable/flag_transparent"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:rotation="90"
                android:layout_gravity="center"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="andrea"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="@dimen/mediumHeadingText"
                    android:textColor="@color/subtitle"
                    />

                <View
                    android:layout_width="50dp"
                    android:layout_height="3dp"
                    android:background="@drawable/flag_transparent"
                    />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:rotation="90"
                android:layout_gravity="center"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="diana"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="@dimen/mediumHeadingText"
                    android:textColor="@color/subtitle"
                    />

                <View
                    android:layout_width="50dp"
                    android:layout_height="3dp"
                    android:background="@drawable/flag_transparent"
                    />

            </LinearLayout>

        </LinearLayout>


        <RelativeLayout
            android:layout_width="350dp"
            android:layout_height="match_parent"
            android:theme="@style/Base.Theme.ChannelManager"
            android:layout_toEndOf="@+id/vertical_linear"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginVertical="40dp"
            android:background="@drawable/relative_layout_backhround"
            android:layout_marginStart="10dp"
            >

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="280dp"
            android:src="@drawable/png_hotel8"
            android:scaleType="fitXY"
            />

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#232325"
                app:cardCornerRadius="5dp"
                android:layout_marginTop="15dp"
                android:layout_marginStart="15dp"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="suit tanya"
                    android:gravity="center"
                    android:layout_margin="5dp"
                    android:textColor="@color/primarybackground"
                    />
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#232325"
                app:cardCornerRadius="5dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="5dp"
                android:layout_marginEnd="10dp"
                >

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="center"
                    android:text="FULL INFO"
                    android:textColor="@color/primarybackground" />
            </androidx.cardview.widget.CardView>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/png_room" />

        </RelativeLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/combine_frame"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/images_frame"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/primarybackground"
        android:elevation="8dp"
        >

        <RelativeLayout
            android:id="@+id/rating_frame"
            android:layout_width="500dp"
            android:layout_height="250dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:background="@color/primarybackground"
            >

            <TextView
                android:id="@+id/rating_heading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="RATING"
                android:fontFamily="@font/inter_medium"
                android:textSize="@dimen/mediumHeadingText"
                android:layout_marginTop="20dp"
                android:textColor="@color/subtitle"
                android:textStyle="bold"
                />

            <TextView
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/rating_heading"
                android:layout_marginTop="23dp"
                android:layout_marginStart="10dp"
                android:text="8.4"
                android:fontFamily="@font/inter"
                android:textColor="@color/primary"
                android:background="@drawable/rating_text"
                android:gravity="center"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rating_heading"
                android:orientation="vertical"
                android:gravity="center"

                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Location"
                        android:textSize="@dimen/mediumHeadingText"
                        android:fontFamily="@font/montserrat"
                        />

                    <com.mackhartley.roundedprogressbar.RoundedProgressBar
                        android:id="@+id/location_rate"
                        android:layout_width="300dp"
                        android:layout_height="10dp"
                        app:rpbBackgroundColor="@color/secondary"
                        android:layout_marginStart="15dp"
                        app:rpbCornerRadius="20dp"
                        app:layout_constraintTop_toBottomOf="@+id/po8yt"
                        app:rpbProgress="70"
                        android:layout_gravity="center"
                        app:rpbProgressColor="@color/primary"
                        app:rpbShowProgressText="false"
                        app:layout_constraintBottom_toBottomOf="parent"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mediumHeadingText"
                        android:text="8.3"
                        android:layout_marginStart="20dp"
                        android:layout_gravity="center"
                        android:fontFamily="@font/inter"
                        android:textColor="@color/subtitle"
                        />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Location"
                        android:textSize="@dimen/mediumHeadingText"
                        android:fontFamily="@font/montserrat"
                        />

                    <com.mackhartley.roundedprogressbar.RoundedProgressBar
                        android:id="@+id/cleaning_rate"
                        android:layout_width="300dp"
                        android:layout_height="10dp"
                        app:rpbBackgroundColor="@color/secondary"
                        android:layout_marginStart="15dp"
                        app:rpbCornerRadius="20dp"
                        app:layout_constraintTop_toBottomOf="@+id/po8yt"
                        app:rpbProgress="70"
                        android:layout_gravity="center"
                        app:rpbProgressColor="@color/primary"
                        app:rpbShowProgressText="false"
                        app:layout_constraintBottom_toBottomOf="parent"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mediumHeadingText"
                        android:text="8.3"
                        android:layout_marginStart="20dp"
                        android:layout_gravity="center"
                        android:fontFamily="@font/inter"
                        android:textColor="@color/subtitle"
                        />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Location"
                        android:textSize="@dimen/mediumHeadingText"
                        android:fontFamily="@font/montserrat"
                        />

                    <com.mackhartley.roundedprogressbar.RoundedProgressBar
                        android:id="@+id/service_rate"
                        android:layout_width="300dp"
                        android:layout_height="10dp"
                        app:rpbBackgroundColor="@color/secondary"
                        android:layout_marginStart="15dp"
                        app:rpbCornerRadius="20dp"
                        app:layout_constraintTop_toBottomOf="@+id/po8yt"
                        app:rpbProgress="70"
                        android:layout_gravity="center"
                        app:rpbProgressColor="@color/primary"
                        app:rpbShowProgressText="false"
                        app:layout_constraintBottom_toBottomOf="parent"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mediumHeadingText"
                        android:text="8.3"
                        android:layout_marginStart="20dp"
                        android:layout_gravity="center"
                        android:fontFamily="@font/inter"
                        android:textColor="@color/subtitle"
                        />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Location"
                        android:textSize="@dimen/mediumHeadingText"
                        android:fontFamily="@font/montserrat"
                        />

                    <com.mackhartley.roundedprogressbar.RoundedProgressBar
                        android:id="@+id/price_rate"
                        android:layout_width="300dp"
                        android:layout_height="10dp"
                        app:rpbBackgroundColor="@color/secondary"
                        android:layout_marginStart="15dp"
                        app:rpbCornerRadius="20dp"
                        app:layout_constraintTop_toBottomOf="@+id/po8yt"
                        app:rpbProgress="70"
                        android:layout_gravity="center"
                        app:rpbProgressColor="@color/primary"
                        app:rpbShowProgressText="false"
                        app:layout_constraintBottom_toBottomOf="parent"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mediumHeadingText"
                        android:text="8.3"
                        android:layout_marginStart="20dp"
                        android:layout_gravity="center"
                        android:fontFamily="@font/inter"
                        android:textColor="@color/subtitle"
                        />

                </LinearLayout>

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:background="@color/primarybackground"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@id/rating_frame">

            <TextView
                android:id="@+id/about_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="About/Description"
                android:textSize="@dimen/SubHeadingSize"
                android:fontFamily="@font/inter_medium"
                android:textColor="#333333"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:textFontWeight="700"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/about_text"
                android:layout_marginTop="10dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginHorizontal="30dp"
                android:letterSpacing="0.1"
                android:text="In hac habitasse platea dictumst. Donec ullamcorper cursus mollis. Aliquam ultrices posuere consectetur. Praesent non convallis nisi, in finibus en enarcu. Nulla vulputate nulla quis accumsan vulputate. Curabitur sit amet eros vitae mauris aliquet euismod. Proin vulputate dui ut elit enam vestibulum laoreet. Mauris ut placerat quam. Praesent volutpat dignissim interdum. Morbi non justo in ipsum ornare ornare vulputate sed metus nam efficitur. Praesent molestie viverra arcu nec blandit. Integer sodales consectetur justo, in ullamcorper…"
                android:fontFamily="@font/inter_medium"
                android:textSize="@dimen/smallText"
                android:textColor="@color/subtitle"
                />

        </RelativeLayout>

    </RelativeLayout>


        <RelativeLayout
            android:id="@+id/room_detail_frame"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:layout_marginTop="25dp"
            android:background="@color/primarybackground"
            android:elevation="8dp"
            app:layout_constraintTop_toBottomOf="@+id/combine_frame"

            tools:layout_editor_absoluteX="0dp">

            <TextView
                android:id="@+id/roomsHeading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/inter_medium"
                android:text="Rooms"
                android:textColor="#333333"
                android:textFontWeight="700"
                android:textSize="@dimen/HeadingSize"
                android:textStyle="bold" />


            <androidx.cardview.widget.CardView
                android:layout_width="450dp"
                android:layout_height="match_parent"
                android:layout_below="@+id/roomsHeading"
                android:layout_alignParentStart="true"
                android:layout_marginVertical="30dp"
                android:layout_marginStart="30dp"
                app:cardCornerRadius="10dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/png_hotel3"
                        android:scaleType="fitXY"
                        />

                    <TextView
                        android:id="@+id/room_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginBottom="80dp"
                        android:fontFamily="@font/inter_bold"
                        android:text="Deluxe Room"
                        android:textColor="@color/primarybackground"
                        android:textSize="@dimen/HeadingSize"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/vector_location_light"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/room_type" />

                    <TextView
                        android:id="@+id/location"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="50dp"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/inter_medium"
                        android:text="Jaipur, India"
                        android:textColor="@color/primarybackground"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/room_type" />

                    <com.github.saran2020.dragrating.DragRatingView
                        android:id="@+id/rating"
                        android:layout_width="100dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="20dp"
                        app:initial_rating="5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/location"
                        app:max_rating="5" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/inter"
                        android:text="(8.6)"
                        android:textColor="@color/primarybackground"
                        android:textSize="@dimen/mediumHeadingText"
                        app:layout_constraintStart_toEndOf="@+id/rating"
                        app:layout_constraintTop_toBottomOf="@+id/location" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="150dp"
                        android:layout_height="60dp"
                        android:layout_marginEnd="-30dp"
                        android:layout_marginBottom="50dp"
                        android:backgroundTint="@color/primary"
                        android:theme="@style/Base.Theme.ChannelManager"
                        app:cardCornerRadius="40dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginTop="5dp"
                                android:fontFamily="@font/inter_medium"
                                android:text="$ 250"
                                android:textColor="@color/secondary"
                                android:textFontWeight="700"
                                android:textSize="@dimen/HeadingSize"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginBottom="5dp"
                                android:fontFamily="@font/inter_medium"
                                android:text="Per Night"
                                android:textColor="@color/secondary"
                                android:textSize="@dimen/smallText"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </com.google.android.material.card.MaterialCardView>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>


            <TextView
                android:id="@+id/photos_heading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="520dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/inter_medium"
                android:text="Photos"
                android:textColor="@color/secondary"
                android:textFontWeight="700"
                android:textSize="@dimen/HeadingSize"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/linear_gallery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/photos_heading"
                android:layout_alignStart="@+id/photos_heading"
                android:layout_marginTop="30dp">

                <ImageView
                    android:layout_width="120dp"
                    android:layout_height="80dp"
                    android:src="@drawable/png_room_pic" />

                <ImageView
                    android:layout_width="120dp"
                    android:layout_height="80dp"
                    android:layout_marginStart="10dp"
                    android:src="@drawable/png_room_pic" />

                <ImageView
                    android:layout_width="120dp"
                    android:layout_height="80dp"
                    android:layout_marginStart="10dp"
                    android:src="@drawable/png_room_pic" />

                <ImageView
                    android:layout_width="120dp"
                    android:layout_height="80dp"
                    android:layout_marginStart="10dp"
                    android:src="@drawable/png_room_pic" />

            </LinearLayout>

            <TextView
                android:id="@+id/location_heading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/linear_gallery"
                android:layout_alignStart="@id/linear_gallery"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/inter_medium"
                android:text="Location"
                android:textColor="@color/secondary"
                android:textSize="@dimen/HeadingSize"
                android:textStyle="bold" />


            <FrameLayout
                android:id="@+id/map_fragment_container"
                android:layout_width="match_parent"
                android:layout_height="230dp"
                android:layout_below="@id/location_heading"
                android:layout_alignStart="@+id/location_heading"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="80dp" />

        </RelativeLayout>


    <RelativeLayout
        android:id="@+id/calender_frame"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        app:layout_constraintTop_toBottomOf="@id/room_detail_frame"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        android:elevation="8dp"
        android:background="@color/primarybackground"
        />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="700dp"
            app:layout_constraintTop_toBottomOf="@id/calender_frame"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="20dp"
            android:elevation="8dp"
            android:background="@color/primarybackground"
            android:layout_marginBottom="60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            >

            <TextView
                android:id="@+id/views_heading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Room 3D Views"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:textSize="@dimen/HeadingSize"
                android:fontFamily="@font/inter_medium"
                android:textStyle="bold"
                android:textColor="@color/textcolor"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/views_heading"
                android:layout_alignParentStart="true"
                android:layout_marginHorizontal="30dp"
                android:gravity="center"
                android:layout_alignParentEnd="true"
                android:background="@color/primarybackground"
                >

                <androidx.cardview.widget.CardView
                    android:id="@+id/panoramaViewCard"
                    android:layout_width="300dp"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="30dp"
                    app:cardCornerRadius="16dp"
                    app:cardBackgroundColor="@color/primarybackground">

                <com.google.android.material.imageview.ShapeableImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/paranomathubnail"

<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/secondarybackground"
    tools:context=".Dashboard.DashboardFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
   <LinearLayout
       android:id="@+id/ll1"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="horizontal"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintEnd_toEndOf="parent"
       >


       <RelativeLayout
           android:id="@+id/booking_frame"
           android:layout_width="700dp"
           android:layout_height="680dp"
           android:layout_marginTop="40dp"
           android:layout_marginStart="15dp"
           app:layout_constraintTop_toTopOf="parent"
           app:layout_constraintStart_toStartOf="parent"
           android:background="#FFFFFF"
           >
           
           <ImageView
               android:id="@+id/icon"
               android:layout_width="30dp"
               android:layout_height="30dp"
               android:layout_marginTop="15dp"
               android:layout_marginStart="20dp"
               android:src="@drawable/vector_booking"
               />

           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="Booking News"
               android:fontFamily="@font/inter_bold"
               android:textSize="20dp"
               android:textColor="@color/secondary"
               android:textFontWeight="600"
               android:layout_marginTop="15dp"
               android:layout_toEndOf="@+id/icon"
               android:layout_marginStart="5dp"
               />

           <com.google.android.material.card.MaterialCardView
               android:layout_width="130dp"
               android:layout_height="40dp"
               android:layout_alignParentEnd="true"
               android:layout_marginEnd="20dp"
               android:layout_marginTop="20dp"
               app:cardElevation="8dp"
               android:background="@color/secondarybackground"
               android:backgroundTint="@color/secondarybackground"
               app:strokeColor="@color/primarybackground"
               >

               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Today"
                   android:textSize="18dp"
                   android:fontFamily="@font/montserrat"
                   android:textFontWeight="200"
                   android:layout_gravity="center"
                    android:textColor="@color/black"
                   />

               <ImageView
                   android:layout_width="20dp"
                   android:layout_height="20dp"
                   android:src="@drawable/vector_dropdown"
                   android:layout_gravity="end"
                   android:layout_marginEnd="10dp"
                   android:layout_marginTop="10dp"
                   />

           </com.google.android.material.card.MaterialCardView>

           <LinearLayout
               android:id="@id/ll1"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:layout_below="@+id/icon"
               android:layout_marginTop="30dp"
               android:layout_marginStart="20dp"
               >

               <androidx.cardview.widget.CardView
                   android:id="@+id/card_booking"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   app:cardCornerRadius="8dp"
                   app:cardElevation="3dp"
                   android:layout_marginVertical="12dp"
                   android:layout_marginStart="12dp"
                   app:cardBackgroundColor="@color/primary">

                   <androidx.cardview.widget.CardView
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       app:cardCornerRadius="8dp"
                       android:layout_marginTop="2dp"
                       app:cardBackgroundColor="@color/white">

                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:orientation="horizontal"
                           android:background="@color/white"
                           android:layout_gravity="center_vertical"
                           android:gravity="center_vertical"
                           android:padding="6dp">

                           <ImageView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:src="@drawable/vector_people"/>

                           <TextView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textColor="@color/black"
                               android:paddingStart="2dp"
                               android:textSize="12dp"
                               android:text="New Bookings"/>

                       </LinearLayout>

                   </androidx.cardview.widget.CardView>

               </androidx.cardview.widget.CardView>



               <androidx.cardview.widget.CardView
                   android:id="@+id/exploreJobs"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   app:cardCornerRadius="8dp"
                   app:cardElevation="3dp"
                   android:visibility="gone"
                   android:layout_marginVertical="12dp"
                   android:layout_marginStart="12dp"
                   app:cardBackgroundColor="@color/white">

                   <androidx.cardview.widget.CardView
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       app:cardCornerRadius="8dp"
                       android:layout_marginTop="2dp"
                       app:cardBackgroundColor="@color/white">

                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:orientation="horizontal"
                           android:background="@color/white"
                           android:layout_gravity="center_vertical"
                           android:gravity="center_vertical"
                           android:padding="6dp">

                           <ImageView
                               android:layout_width="17dp"
                               android:layout_height="20dp"
                               android:src="@drawable/vector_people"/>

                           <TextView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textColor="@color/black"
                               android:paddingStart="2dp"
                               android:textSize="12dp"
                               android:text="Jobs"/>

                       </LinearLayout>

                   </androidx.cardview.widget.CardView>

               </androidx.cardview.widget.CardView>

               <androidx.cardview.widget.CardView
                   android:id="@+id/card_arrival"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   app:cardCornerRadius="8dp"
                   app:cardElevation="3dp"
                   android:layout_marginVertical="12dp"
                   android:layout_marginStart="12dp"
                   app:cardBackgroundColor="@color/white">

                   <androidx.cardview.widget.CardView
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       app:cardCornerRadius="8dp"
                       android:layout_marginTop="2dp"
                       app:cardBackgroundColor="@color/white">

                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:orientation="horizontal"
                           android:background="@color/white"
                           android:layout_gravity="center_vertical"
                           android:gravity="center_vertical"
                           android:padding="6dp">

                           <ImageView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:src="@drawable/vector_people"/>

                           <TextView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textColor="@color/black"
                               android:paddingStart="2dp"
                               android:textSize="12dp"
                               android:text="Arrivals"/>

                       </LinearLayout>

                   </androidx.cardview.widget.CardView>

               </androidx.cardview.widget.CardView>

               <androidx.cardview.widget.CardView
                   android:id="@+id/card_departure"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   app:cardCornerRadius="8dp"
                   app:cardElevation="3dp"
                   android:layout_marginVertical="12dp"
                   android:layout_marginStart="12dp"
                   app:cardBackgroundColor="@color/white">

                   <androidx.cardview.widget.CardView
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       app:cardCornerRadius="8dp"
                       android:layout_marginTop="2dp"
                       app:cardBackgroundColor="@color/white">

                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:orientation="horizontal"
                           android:background="@color/white"
                           android:layout_gravity="center_vertical"
                           android:gravity="center_vertical"
                           android:padding="6dp">

                           <ImageView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:src="@drawable/vector_people"/>

                           <TextView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textColor="@color/black"
                               android:paddingStart="2dp"
                               android:textSize="12dp"
                               android:text="Departures"/>

                       </LinearLayout>

                   </androidx.cardview.widget.CardView>

               </androidx.cardview.widget.CardView>



               <androidx.cardview.widget.CardView
                   android:id="@+id/cancellations"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   app:cardCornerRadius="8dp"
                   app:cardElevation="3dp"
                   android:layout_marginVertical="12dp"
                   android:layout_marginStart="12dp"
                   app:cardBackgroundColor="@color/white">

                   <androidx.cardview.widget.CardView
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       app:cardCornerRadius="8dp"
                       android:layout_marginTop="2dp"
                       app:cardBackgroundColor="@color/white">

                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:orientation="horizontal"
                           android:background="@color/white"
                           android:layout_gravity="center_vertical"
                           android:gravity="center_vertical"
                           android:padding="6dp">

                           <ImageView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:src="@drawable/vector_people"/>

                           <TextView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textColor="@color/black"
                               android:paddingStart="2dp"
                               android:textSize="12dp"
                               android:text="Cancellations"/>

                       </LinearLayout>

                   </androidx.cardview.widget.CardView>

               </androidx.cardview.widget.CardView>


           </LinearLayout>

           <View
               android:id="@+id/view"
               android:layout_width="match_parent"
               android:layout_height="1dp"
               android:layout_below="@+id/ll1"
               android:background="#828282"
               android:layout_alignParentStart="true"
               android:layout_marginStart="20dp"
               android:layout_alignParentEnd="true"
               android:layout_marginEnd="40dp"
               />

           <FrameLayout
               android:id="@+id/fragment_container_booking"
               android:layout_width="match_parent"
               android:layout_height="match_parent"
               android:layout_below="@+id/view"
               android:layout_marginTop="10dp"
               />

       </RelativeLayout>

       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="680dp"
           app:layout_constraintTop_toTopOf="parent"
           app:layout_constraintStart_toEndOf="@+id/booking_frame"
           android:layout_marginStart="10dp"
           android:layout_marginTop="40dp"
           app:layout_constraintEnd_toEndOf="parent"
           android:orientation="vertical"
           >

           <com.google.android.material.card.MaterialCardView
               android:layout_width="match_parent"
               android:layout_height="200dp"
               android:backgroundTint="@color/primarybackground"
               app:cardElevation="20dp"
               android:layout_marginHorizontal="10dp"
               >

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   
                   <TextView
                       android:id="@+id/heading"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toTopOf="parent"
                       app:layout_constraintStart_toStartOf="parent"
                       android:text="Average Monthly Income"
                       android:layout_marginTop="30dp"
                       android:textColor="@color/textcolor"
                       android:fontFamily="@font/inter_medium"
                       android:textSize="@dimen/subTitle"
                       android:layout_marginStart="20dp"
                       />

                   <TextView
                       android:id="@+id/average_income"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toBottomOf="@+id/heading"
                       android:text="â‚¹ 132,098"
                       app:layout_constraintStart_toStartOf="@+id/heading"
                       android:layout_marginTop="10dp"
                       android:textSize="30dp"
                       android:fontFamily="@font/inter_medium"
                       android:textColor="@color/textcolor"
                       />

                   <com.google.android.material.card.MaterialCardView
                       android:id="@+id/change_card"
                       android:layout_width="60dp"
                       android:layout_height="25dp"
                       app:layout_constraintTop_toBottomOf="@+id/average_income"
                       app:layout_constraintStart_toStartOf="@+id/average_income"
                       android:layout_marginTop="10dp"
                       android:backgroundTint="#80ACD037"
                       >

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="10.1%"
                           android:layout_gravity="center"
                           android:fontFamily="@font/inter_bold"
                           android:textColor="@color/black"
                           />

                   </com.google.android.material.card.MaterialCardView>

                   <TextView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toTopOf="@+id/change_card"
                       app:layout_constraintStart_toEndOf="@+id/change_card"
                       app:layout_constraintBottom_toBottomOf="@+id/change_card"
                       android:text="vs 2 previous months"
                       android:textColor="@color/subtitle"
                       android:layout_marginStart="5dp"
                       android:fontFamily="@font/inter"
                       android:textSize="12dp"
                       />

                   <com.github.mikephil.charting.charts.LineChart
                       android:id="@+id/lineChart"
                       android:layout_width="300dp"
                       android:layout_height="150dp"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintTop_toTopOf="parent"
                       app:layout_constraintBottom_toBottomOf="parent"
                       android:layout_margin="10dp"
                       />

               </androidx.constraintlayout.widget.ConstraintLayout>

           </com.google.android.material.card.MaterialCardView>

           <com.google.android.material.card.MaterialCardView
               android:layout_width="match_parent"
               android:layout_height="235dp"
               android:backgroundTint="@color/primarybackground"
               app:cardElevation="20dp"
               android:layout_marginTop="20dp"
               android:layout_marginHorizontal="10dp"
               >

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">

                   <TextView
                       android:id="@+id/heading_pie"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_marginStart="20dp"
                       android:layout_marginTop="50dp"
                       android:fontFamily="@font/inter_medium"
                       android:text="Rooms Availability"
                       android:textColor="@color/textcolor"
                       android:textSize="@dimen/subTitle"
                       app:layout_constraintStart_toStartOf="parent"
                       app:layout_constraintTop_toTopOf="parent" />


                   <com.github.mikephil.charting.charts.PieChart
                       android:id="@+id/pie_chart"
                       android:layout_width="320dp"
                       android:layout_height="260dp"
                       android:layout_margin="10dp"
                       app:layout_constraintBottom_toBottomOf="parent"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintTop_toTopOf="parent" />

               </androidx.constraintlayout.widget.ConstraintLayout>

           </com.google.android.material.card.MaterialCardView>

           <com.google.android.material.card.MaterialCardView
               android:layout_width="match_parent"
               android:layout_height="200dp"
               android:backgroundTint="@color/primarybackground"
               app:cardElevation="20dp"
               android:layout_marginHorizontal="10dp"
               android:layout_marginTop="20dp"
               >

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">

                   <TextView
                       android:id="@+id/ota_heading"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toTopOf="parent"
                       app:layout_constraintStart_toStartOf="parent"
                       app:layout_constraintEnd_toEndOf="parent"
                       android:layout_marginTop="20dp"
                       android:text="Best Source, Room, Rate &amp; OTA "
                       android:fontFamily="@font/dm_sans"
                       android:textSize="25dp"
                       android:textColor="@color/textcolor"
                       android:paddingLeft="20dp"
                       />

                   <LinearLayout
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toBottomOf="@+id/ota_heading"
                       android:layout_marginTop="15dp"
                       app:layout_constraintBottom_toBottomOf="parent"
                       android:layout_marginBottom="10dp"
                       android:orientation="horizontal"
                       app:layout_constraintStart_toStartOf="parent"
                       app:layout_constraintEnd_toEndOf="parent"
                       >

                       <ImageView
                           android:layout_width="110dp"
                           android:layout_height="120dp"
                           android:src="@drawable/png_ota1"
                           android:layout_marginStart="20dp"
                           />

                       <ImageView
                           android:layout_width="110dp"
                           android:layout_height="120dp"
                           android:src="@drawable/png_ota2"
                           android:layout_marginStart="20dp"
                           />
                       <ImageView
                           android:layout_width="110dp"
                           android:layout_height="120dp"
                           android:src="@drawable/png_ota3"
                           android:layout_marginStart="20dp"
                           />
                       <ImageView
                           android:layout_width="110dp"
                           android:layout_height="120dp"
                           android:src="@drawable/png_ota4"
                           android:layout_marginStart="20dp"
                           />

                   </LinearLayout>

               </androidx.constraintlayout.widget.ConstraintLayout>

           </com.google.android.material.card.MaterialCardView>
       </LinearLayout>

   </LinearLayout>


    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/ll1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/icon_property"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/vector_property_details"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                />

            <TextView
                android:id="@+id/property_heading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@+id/icon_property"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="10dp"
                android:text="Property Statistics"
                android:textSize="25dp"
                android:fontFamily="@font/inter_bold"
                android:textFontWeight="600"
                android:textColor="@color/textcolor"
                android:textStyle="bold"
                />
            <LinearLayout
                android:id="@id/ll1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintTop_toBottomOf="@+id/property_heading"
                android:layout_marginTop="30dp"
                android:layout_marginStart="20dp"
                >

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_overview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="3dp"
                    android:layout_marginVertical="12dp"
                    android:layout_marginStart="12dp"
                    app:cardBackgroundColor="@color/primary">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardCornerRadius="8dp"
                        android:layout_marginTop="2dp"
                        app:cardBackgroundColor="@color/white">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@color/white"
                            android:layout_gravity="center_vertical"
                            android:gravity="center_vertical"
                            android:padding="6dp">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/vector_people"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:paddingStart="2dp"
                                android:textSize="12dp"
                                android:text="Today's Overview"/>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </androidx.cardview.widget.CardView>





                <androidx.cardview.widget.CardView
                    android:id="@+id/card_revenue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="3dp"
                    android:layout_marginVertical="12dp"
                    android:layout_marginStart="12dp"
                    app:cardBackgroundColor="@color/white">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardCornerRadius="8dp"
                        android:layout_marginTop="2dp"
                        app:cardBackgroundColor="@color/white">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@color/white"
                            android:layout_gravity="center_vertical"
                            android:gravity="center_vertical"
                            android:padding="6dp">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/vector_people"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:paddingStart="2dp"
                                android:textSize="12dp"
                                android:text="Revenue"/>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_Statistics"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="3dp"
                    android:layout_marginVertical="12dp"
                    android:layout_marginStart="12dp"
                    app:cardBackgroundColor="@color/white">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardCornerRadius="8dp"
                        android:layout_marginTop="2dp"
                        app:cardBackgroundColor="@color/white">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@color/white"
                            android:layout_gravity="center_vertical"
                            android:gravity="center_vertical"
                            android:padding="6dp">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/vector_people"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:paddingStart="2dp"
                                android:textSize="12dp"
                                android:text="Booking Statistics"/>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </androidx.cardview.widget.CardView>






            </LinearLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/ll1"
                android:background="#828282"
                app:layout_constraintTop_toBottomOf="@+id/ll1"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                />

            <FrameLayout
                android:id="@+id/fragmant_container_property"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/view2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="20dp"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>

    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.core.widget.NestedScrollView>